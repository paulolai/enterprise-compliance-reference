{
  "name": "@executable-specs/domain",
  "version": "1.0.0",
  "type": "module",
  "description": "Core pricing engine with property-based tests",
  "main": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./pricing-engine": "./src/pricing-engine.ts",
    "./types": "./src/types.ts",
